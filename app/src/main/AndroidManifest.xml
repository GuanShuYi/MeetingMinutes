<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gattung.biz.meetingminutes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AgendaItemEditorActivity"
            android:label="Edit Agenda Item"
            android:parentActivityName=".AgendaItemListActivity">
        </activity>

        <activity
            android:name=".AgendaItemListActivity"
            android:label="Agenda Items"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".EditAgendaActivity"
            android:label="@string/title_activity_edit_agenda"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>